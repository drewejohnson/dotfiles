# Config for pretty plots
mathtext.fontset : stix
font.family : STIXGeneral
font.size : 12
# axes
axes.titlesize : medium
# coloring is seaborn-colorblind, found in matplotlib defaults
axes.prop_cycle : cycler(color=['0173b2', 'de8f05', '029e73', 'd55e00', 'cc78bc', 'ca9a61'], marker=['o', 'x', 's', 'h', '*', 'd'])
